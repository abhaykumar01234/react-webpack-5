  "files": [
    "/dist"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:abhaykumar01234/react-webpack-5.git",
    "directory": "/"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
